extends shared/_dashmaster
include shared/_editor_navigation

block body
    main(class="row")
    +editor_navigation
    div(class="col-sm-10")
        div(id="editor" )
            form(action="/dashboard" method='post' enctype="multipart/form-data")
                input(id="title" name="title" placeholder="Title" style="display:block")
                input(name="categories" placeholder="Categories" style="display:block")
                input(name="tags" placeholder="Tags" style="display:block")
                textarea(name="content" placeholder="Text" style="display:block")
                input(type="file" name="file")
                button(type="submit")
                    | Submit